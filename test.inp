&GLOBAL	    
   PROJECT     hematite    
   RUN_TYPE    GEO_OPT    
&END GLOBAL	    

&MOTION	    
   &GEO_OPT	    
      OPTIMIZER    BFGS    
      MAX_ITER     1000    
   &END GEO_OPT	    

   &PRINT	    
      &TRAJECTORY	    
         FORMAT    DCD    
      &END TRAJECTORY	    
   &END PRINT	    
&END MOTION	    

&FORCE_EVAL	    
   METHOD                      Quickstep    
   STRESS_TENSOR               ANALYTICAL    

   &DFT	    
      UKS                         T    
      CHARGE                      -1    
      # MULTIPLICITY 2
      BASIS_SET_FILE_NAME         ../input/BASIS_ADMM    
      BASIS_SET_FILE_NAME         ../input/BASIS_MOLOPT    
      POTENTIAL_FILE_NAME         ../input/GTH_POTENTIALS    

      &AUXILIARY_DENSITY_MATRIX_METHOD	    
         METHOD                      BASIS_PROJECTION    
         ADMM_PURIFICATION_METHOD    MO_DIAG    
      &END AUXILIARY_DENSITY_MATRIX_METHOD	    

      &MGRID	    
         CUTOFF    400    
      &END MGRID	    

      &QS	    
         EPS_DEFAULT    1.00E-12    
      &END QS	    

      &SCF	    
         SCF_GUESS         ATOMIC    
         EPS_SCF           1.0E-5    
         MAX_SCF           40    

         &OT	    
            PRECONDITIONER    FULL_SINGLE_INVERSE    
            MINIMIZER         DIIS    
         &END OT	    

         &OUTER_SCF	    
            MAX_SCF    60    
            EPS_SCF    1.0E-5    
         &END OUTER_SCF	    
      &END SCF	    

      &XC	    
         &VDW_POTENTIAL	    
            POTENTIAL_TYPE          PAIR_POTENTIAL    

            &PAIR_POTENTIAL	    
               TYPE                    DFTD3    
               REFERENCE_FUNCTIONAL    PBE0    
               PARAMETER_FILE_NAME     ../input/dftd3.dat    
            &END PAIR_POTENTIAL	    
         &END VDW_POTENTIAL	    

         &XC_FUNCTIONAL	    
            &PBE	    
               SCALE_X    0.0    
               SCALE_C    1.0    
            &END PBE	    

            &XWPBE	    
               SCALE_X     -0.12    
               SCALE_X0    1.0    
               OMEGA       0.11    
            &END XWPBE	    
         &END XC_FUNCTIONAL	    

         &HF	    
            &SCREENING	    
               EPS_SCHWARZ            1.0E-4    
               SCREEN_ON_INITIAL_P    FALSE    
            &END SCREENING	    

            &INTERACTION_POTENTIAL	    
               POTENTIAL_TYPE    SHORTRANGE    
               OMEGA             0.11    
            &END INTERACTION_POTENTIAL	    

            &MEMORY	    
               MAX_MEMORY             2400    
               EPS_STORAGE_SCALING    0.1    
            &END MEMORY	    
            FRACTION               0.12    
         &END HF	    
      &END XC	    

      &PRINT	    
         &DERIVATIVES	ON    
         &END DERIVATIVES	    

         &E_DENSITY_CUBE	ON    
         &END E_DENSITY_CUBE	    
      &END PRINT	    
   &END DFT	    

